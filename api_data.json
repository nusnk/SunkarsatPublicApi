[  {    "type": "post",    "url": "/users",    "title": "Register a new user",    "version": "1.0.0",    "group": "Users",    "header": {      "fields": {        "Header": [          {            "group": "Header",            "optional": false,            "field": "Content-Type",            "defaultValue": "application/x-www-form-urlencoded",            "description": ""          }        ]      }    },    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "email",            "description": "<p>User email</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "first_name",            "description": "<p>User first name, restriction: min:1 max:32</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "last_name",            "description": "<p>User last name, restriction: min:1 max:32</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "company",            "description": "<p>User company, restriction:  max:64</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "purpose",            "description": "<p>User company, restriction:  max:500</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "locale",            "description": "<p>UI language, possible values: <code>ru</code>, <code>kz</code>, <code>en</code>. Default: <code>ru</code></p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "password",            "description": "<p>User password: <br> *should contain at least one digit <br> *should contain at least one lower case <br> *should contain at least one upper case <br> *should contain at least 8 from the mentioned characters</p>"          }        ]      },      "examples": [        {          "title": "body",          "content": "{\n  \"email\":\"nurbolatsnk@gmail.com\",\n  \"first_name\": \"Nurbolat\",\n  \"last_name\": \"Sansyzbay\",\n  \"password\": \"Nurbolat123\"\n}",          "type": "json"        }      ]    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Boolean",            "optional": false,            "field": "success",            "description": "<p>User registration status</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "message",            "description": "<p>Message description</p>"          }        ]      },      "examples": [        {          "title": "Success",          "content": "HTTP/1.1 200 OK\n{\n    \"success\": true,\n    \"message\": \"VerifyEmailAddress\",\n }",          "type": "json"        }      ]    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "type": "Boolean",            "optional": false,            "field": "success",            "description": "<p>User registration status</p>"          },          {            "group": "Error 4xx",            "type": "Object",            "optional": false,            "field": "error",            "description": "<p>Error object</p>"          },          {            "group": "Error 4xx",            "type": "String",            "optional": false,            "field": "error.message",            "description": "<p>The error message, possible values: <br> <code>InvalidEmail</code> <br> <code>InvalidPassword</code> <br> <code>InvalidFirstName</code> <br> <code>InvalidLastName</code> <br> <code>InvalidLocale</code> <br> <code>InvalidCompany</code> <br> <code>InvalidPurpose</code> <br> <code>InvalidLocale</code> <br> <code>UserAlreadyExists</code> <br> <code>VerifyEmail</code> <br> <code>InternalServer</code> - Internal server error <br> *<code>parameterName</code> - will return if someone pass non accepted parameters</p>"          }        ]      },      "examples": [        {          "title": "Error-Response:",          "content": "HTTP/1.1 400 Bad Request\n{   \n \"success\": false,\n  \"error\": {\n     message: \"InvalidPassword\"\n    }\n}",          "type": "json"        }      ]    },    "filename": "src/@1.0.0/api-public/areas.controller.js",    "groupTitle": "Users",    "name": "PostUsers"  }]
